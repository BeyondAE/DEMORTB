extends layout

block content
  div.container
    - var col = ['col-sm-6','col-sm-offset-3']
    div(class=col)
      h1 <span class='fa fa-upload'></span> File Upload
      form(action='/upload', method='post', enctype='multipart/form-data')
        div.form-group
          label Title
          input(type='text',class='form-control',name='title')
        div.form-group
          label Files
          input(type='file',name='pict',multiple='multiple')
        - var btn = ['btn', 'btn-warning', 'btn-lg']
        button(type='submit', class=btn) Upload